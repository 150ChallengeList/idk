                dropdownItemButtonInactive.TextXAlignment = Enum.TextXAlignment.Left]]--

                --task.spawn(callback, DropdownIndicatorText.Text)

                dropdownButton.MouseButton1Click:Connect(function()
                    if dropdownItemHolder.Visible ~= true then
                        dropdownItemHolder.Visible = true
                        local TweenService = game:GetService("TweenService")

                        local onsize = UDim2.new(0, 206, 0, size1)

                        local onanim = TweenService:Create(dropdownItemHolder, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {Size = onsize})

                        onanim:Play()

                        local TweenService = game:GetService("TweenService")

                        local onrotation = 180.00

                        local onanim = TweenService:Create(dropdownArrow, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {Rotation = onrotation})

                        onanim:Play()
                    elseif dropdownItemHolder.Visible ~= false then
                        local TweenService = game:GetService("TweenService")

                        local offsize = UDim2.new(0, 206, 0, 0)

                        local offanim = TweenService:Create(dropdownItemHolder, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {Size = offsize})

                        offanim:Play()


                        local TweenService = game:GetService("TweenService")

                        local offrotation = 90.00

                        local offanim = TweenService:Create(dropdownArrow, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out), {Rotation = offrotation})

                        offanim:Play()

                        task.wait(0.1)

                        dropdownItemHolder.Visible = false
                    end
                    --[[if tableIndex == #opts then
                        tableIndex = 1
                        selectedoptiontext.Text = opts[tableIndex]
                    else
                        tableIndex = tableIndex + 1
                        selectedoptiontext.Text = opts[tableIndex]
                    end]]--


