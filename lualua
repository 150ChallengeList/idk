                    dropdownItemButton.MouseButton1Click:Connect(function()
                        state = dropdownItemButton.Text
                        tableIndex = #opts
                        for i, v in pairs(dropdownItemHolder:GetChildren()) do
                            for q, k in pairs(v:GetChildren()) do
                                if k:IsA("TextButton") then
                                    k.TextColor3 = Color3.fromRGB(159, 153, 157)
                                end
                            end
                        end
                        task.spawn(callback, state)
                        DropdownIndicatorText.Text = dropdownItemButton.Text
                        dropdownItemButton.TextColor3 = Color3.fromRGB(185, 86, 122)
                    end)
                end

                --for when the dropdown is active the items lmao
                

